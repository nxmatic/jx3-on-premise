apiVersion: config.k8s.io/v1alpha1
kind: Krmfile
openAPI:
  definitions:
    io.k8s.cli.setters.key:
      x-k8s-cli:
        setter:
          name: key
          value: ssh-rsa   nuxeo@MacBook-Pro-de-Stephane.local
    io.k8s.cli.setters.timezone:
      x-k8s-cli:
        setter:
          name: timezone
          value: Europe/Paris
    io.k8s.cli.substitutions.username-binding:
      x-k8s-cli:
        substitution:
          name: username-binding
          pattern: username=${user}
          values:
            - marker: ${user}
              ref: '#/definitions/io.k8s.cli.setters.user'
    io.k8s.cli.setters.user:
      x-k8s-cli:
        setter:
          name: user
          value: nuxeo
